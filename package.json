{
  "name": "bookreads-backend",
  "version": "1.0.0",
  "description": "Backend para red social de lectores",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon",
    "dev:simple": "ts-node src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "start:railway": "npm run prisma:generate && npm run prisma:migrate:deploy && npm run db:init && npm run db:indices && npm start",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:migrate:resolve": "prisma migrate resolve --applied",
    "prisma:migrate:status": "prisma migrate status",
    "prisma:db:push": "prisma db push",
    "prisma:db:push:force": "prisma db push --force-reset",
    "prisma:studio": "prisma studio",
    "seed": "ts-node src/scripts/seedDatabase.ts",
    "seed:clean": "ts-node src/scripts/seedDatabase.ts --clean",
    "db:clean": "ts-node src/scripts/cleanDatabase.ts",
    "db:reset": "npm run db:clean && npm run seed",
    "db:init": "ts-node src/scripts/initDatabase.ts",

    "db:indices": "ts-node src/scripts/quickOptimize.ts",
    "db:indices:full": "ts-node src/scripts/optimizeBookSearch.ts",
    "test:books": "ts-node src/scripts/testGoogleBooks.ts",
    "test:search": "ts-node src/scripts/testPreciseSearch.ts",
    "test:search:stress": "ts-node src/scripts/testPreciseSearch.ts --stress",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix"
  },
  "dependencies": {
    "@prisma/client": "^5.14.0",
    "axios": "^1.9.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "express-rate-limit": "^7.2.0",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "winston": "^3.13.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.4",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.8.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.12.12",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "prisma": "^5.14.0",
    "ts-jest": "^29.1.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "keywords": [
    "express",
    "typescript",
    "prisma",
    "books",
    "social-network"
  ],
  "author": "lechuit",
  "license": "MIT"
}
